#!/usr/bin/env python3

import sys


if (sys.version_info < (3, 3)):
    sys.path.append('/lib/python3.2/site-packages')

import heartbeat


if __name__ == "__main__":
    try:
        heartbeat.main()
    except (KeyboardInterrupt):
        print("Caught ctrl+c, shutting down Heartbeat...")
        sys.exit(0)
